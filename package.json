{
  "name": "@maciejmiklas/ngx-bootstrap-slider-app",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-bootstrap-slider --configuration production",
    "npm_pack": "cd projects/ngx-bootstrap-slider/dist && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "npm run package && cd projects/ngx-bootstrap-slider/dist && npm publish",
    "package:ngx-bootstrap-slider": "npm run build_lib && cd dist/ngx-bootstrap-slider && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.1",
    "@angular/common": "^15.1.1",
    "@angular/compiler": "^15.1.1",
    "@angular/core": "^15.1.1",
    "@angular/forms": "^15.1.1",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.1.1",
    "@angular/router": "^15.1.1",
    "bootstrap-slider": "^11.0.2",
    "core-js": "^3.27.2",
    "rxjs": "^7.8.0",
    "tslib": "*",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^15.1.2",
    "@angular/compiler-cli": "^15.1.1",
    "@angular/language-service": "^15.1.1",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.11.18",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^4.5.0",
